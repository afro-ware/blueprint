{
    "name": "afroware/blueprint",
    "description": "API Blueprint documentation generator.",
    "keywords": ["blueprint", "api", "laravel", "afroware", "docs"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Adel Lamallam",
            "email": "lamallam.adel@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.5",
        "illuminate/filesystem": "^5.5",
        "doctrine/annotations": "^1.5",
        "phpdocumentor/reflection-docblock" : "^4.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "phpunit/phpunit": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Afroware\\Blueprint\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Afroware\\Blueprint\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
